	"pluginsConfig": {
		"insert-logo": {
			"url": "./images/logo.png",
			"style": "background: none; max-height: 30px; min-height: 30px"
		},
		"github": {
            "url": "https://github.com/kktoo"
        },
		 "sharing": {
           "douban": false,
           "facebook": false,
           "google": false,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": true,
           "qzone": true,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "douban", "weibo", "qq", "qzone"
           ]
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		},
		"favicon": "./images/favicon.ico",
		"donate": {
